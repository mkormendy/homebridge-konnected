    "type": "git",
    "url": "git+https://github.com/konnected-io/homebridge-konnected.git"
  },
  "bugs": {
    "url": "https://github.com/konnected-io/homebridge-konnected/issues"
  },
  "engines": {
    "node": ">=12.0.0",
    "homebridge": ">=1.1.1"
  },
  "main": "dist/index.js",
