  "dependencies": {
    "express": "^4.17.1",
    "ip": "^1.1.5",
    "node-fetch": "^2.6.1",
    "node-ssdp": "^4.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/ip": "^1.1.0",
    "@types/node": "^16.0.1",
